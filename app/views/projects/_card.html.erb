<% @project.points.each do |point| %>
    <% if point.status == number %>
      <div class="col-lg-12 well well-sm">
        <p class=" text-center">
          <% if point.status==1 %>
          <a  data-toggle="collapse" href="#collapseOne<%= point.id %>" >
            <span class="label label-default">
          <% end %>
          <% if point.status==2 %>
          <a  data-toggle="collapse" href="#collapseOne<%= point.id %>" >
            <span class="label label-primary">
          <% end %>
          <% if point.status==3 %>
          <a  data-toggle="collapse" href="#collapseOne<%= point.id %>" >
            <span class="label label-info">
          <% end %>
           <% if point.status==4 %>
           <a  data-toggle="collapse" href="#collapseOne<%= point.id %>" >
            <span class="label label-success">
          <% end %>
              
                <%= point.title %>
              
            </span>
            </a>
          </p>
          <p>
            <div id="collapseOne<%= point.id %>" class="accordion-body collapse">
              <%= point.description %>
            </div>
        </p>
      </div>
    <% end %>
  <% end %>